{
  "name": "vite-plugin-local-mock",
  "version": "0.1.0",
  "author": "jiang025",
  "description": "vite plugin for local mock",
  "repository": "https://github.com/jiang025/vite-plugin-local-mock.git",
  "files": [
    "dist"
  ],
  "keywords": [
    "frontend",
    "vite",
    "vite-plugin",
    "vite-plugin-mock",
    "vite-plugin-local-mock",
    "mock",
    "local-mock",
    "local-mock-plugin",
    "local-mock-plugin-vite"
  ],
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "license": "MIT",
  "dependencies": {
    "query-string": "^8.1.0",
    "raw-body": "^2.5.2",
    "regexparam": "^3.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  },
  "scripts": {
    "test": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rm -rf dist && tsup",
    "watch": "tsup --watch",
    "pub": "npm version prerelease && npm run build && npm publish",
    "pub:stable": "npm run build && npm publish"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "sourcemap": false
  }
}